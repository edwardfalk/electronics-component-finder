{
  "name": "electronic-components-finder",
  "version": "0.1.0",
  "description": "A tool to find and compare electronic components across multiple online shops",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:backend": "node backend/dist/server.js",
    "dev": "vite",
    "dev:server": "nodemon --exec ts-node src/index.ts",
    "dev:backend": "ts-node-dev --respawn --transpile-only backend/src/server.ts",
    "build": "tsc && vite build",
    "build:server": "tsc",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:all": "npm run build && npm run build:server && npm run build:backend",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:backend": "eslint backend --ext .ts",
    "preview": "vite preview",
    "test": "jest",
    "test:backend": "jest --config backend/jest.config.js",
    "db:init": "ts-node src/scripts/db-init.ts",
    "db:migrate": "ts-node src/scripts/migrate-data.ts"
  },
  "keywords": [
    "electronics",
    "components",
    "price-comparison",
    "shopping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@tanstack/react-query": "^5.20.5",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "puppeteer": "^22.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.2",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}
